{
  "options": {
    "paths": {
      "root": "",
      "styles": "styles",
      "mbtiles": "data",
      "pmtiles": "data"
    },
    "domains": [
      "localhost:8080",
      "127.0.0.1:8080"
    ],
    "formatQuality": {
      "jpeg": 80,
      "webp": 90
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": false,
    "serveAllStyles": true,
    "serveStaticMaps": true,
    "allowRemoteMarkerIcons": true,
    "allowInlineMarkerImages": true,
    "staticAttributionText": "© OpenMapTiles  © OpenStreetMaps",
    "tileMargin": 0
  },
  "sources": {
    "openmaptiles": {
      "url": "mbtiles://mass.mbtiles",
      "type": "vector"
    }
  },
  "styles": {
    "dark": {
      "style": "dark.json",
      "tilejson": {
        "format": "png"
      }
    },
     "basic": {
      "style": "basic.json",
      "tilejson": {
        "format": "png"
      }
    },
    "other?": {
      "style": "style.json"
    }
 },
  "data": {
    "openmaptiles": {
      "mbtiles": "mass.mbtiles"
    }
  }
}
